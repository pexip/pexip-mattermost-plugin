{
  "name": "pexip-video-connect",
  "author": "Marcos Cereijo",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "webpack --mode=production",
    "lint": "eslint --ignore-pattern node_modules --ignore-pattern dist --ext .js --ext .jsx --ext tsx --ext ts . --quiet --cache",
    "check-types": "tsc --noEmit",
    "test": "jest --silent",
    "update-selfie-segmenter": "node ./update-segmenter-files.js"
  },
  "dependencies": {
    "@pexip/components": "16.15.1",
    "@pexip/infinity": "^17.8.0",
    "@pexip/media-components": "^23.0.0",
    "@pexip/media-processor": "^18.4.0",
    "babel-plugin-typescript-to-proptypes": "2.1.0",
    "mattermost-redux": "^5.33.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/sdp-transform": "2.4.9",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "5.0.2",
    "core-js": "^3.37.1",
    "css-loader": "^7.1.2",
    "eslint": "8.56.0",
    "eslint-config-love": "^47.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "7.34.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "ts-jest": "29.1.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@contexts/(.*)$": "<rootDir>/src/contexts/$1"
    }
  }
}
